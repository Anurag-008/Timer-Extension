{
	"name": "Timer for LeetCode",
	"version": "1",
	"manifest_version": 2,
	"browser_action": {
		"default_icon": "images/logo.png"
	},
	"web_accessible_resources": [
		"images/icons/*.svg"
	],
	"content_scripts": [
		{
			"matches": [
				"<all_urls>"
			],
			"css": [
				"style.css"
			],
			"js": [
				"Timer.js",
				"timerBox.js",
				"drag.js",
				"main.js"
			]
		}
	],
	"permissions": [
		"activeTab"
	]
}